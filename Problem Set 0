\\ Problem Set 0 >> 1

int main(){
printf(" ***********             ***              *            *              \n");
printf(" *         *          *       *          ***          * *             \n");
printf(" *         *         *         *        *****        *   *            \n");
printf(" *         *         *         *          *         *     *           \n");
printf(" *         *         *         *          *        *       *          \n");
printf(" *         *         *         *          *         *     *           \n");
printf(" *         *         *         *          *          *   *            \n");
printf(" *         *          *       *           *           * *             \n");
printf(" ***********             ***              *            *              \n");

return 0;
}

\\ Problem Set 0 >> 2

