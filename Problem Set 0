\\ Problem Set 0 >> 3

#include <stdio.h>
int main(){
printf("This is a C program.\n\n");
printf("This is a C\nprogram.\n\n");
printf("This is a C\n");
printf("program.\n\n");
printf("This\nis \na\nC\nprogram.\n\n");
printf("This\tis\ta\tC\tprogram.\n\n");

return 0 ;
}

\\ Problem Set 0 >> 4

#include <stdio.h>
int main(){
printf("1\n");
printf("2 3\n");
printf("4 5 6\n");
printf("7 8 9 10\n\n");

printf("       1\n");
printf("    2  3\n");
printf("  4 5  6\n");
printf("7 8 9 10\n");
return 0 ;
}
